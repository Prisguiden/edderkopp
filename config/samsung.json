{
    "key": "samsung",
    "type": "manufacturer",
    "url": "http://samsung.com/no",
    "targets": [
        {
            "type": "product",
            "pageId": "#content .product-info-wrap",
            "fetch": [
                {
                    "type": "container",
                    "elem": "#content .product-info-wrap",
                    "name": "info",
                    "children": [
                        {
                            "type": "data",
                            "elem": ".product-title[itemprop=name]",
                            "name": "title"
                        },
                        {
                            "type": "data",
                            "elem": ".product-code[itemprop=identifier]",
                            "attr": "content",
                            "name": "model"
                        }
                    ]
                },
                {
                    "type": "container",
                    "elem": "#content .tech-spec-module .spec-list",
                    "name": "sections",
                    "children": [
                        {
                            "type": "data",
                            "elem": ".tit",
                            "name": "section"
                        },
                        {
                            "type": "container",
                            "elem": ".spec",
                            "name": "specs",
                            "children": [
                                {
                                    "type": "data",
                                    "elem": ".sub-tit",
                                    "name": "spec"
                                },
                                {
                                    "type": "data",
                                    "elem": ".desc",
                                    "name": "value"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
