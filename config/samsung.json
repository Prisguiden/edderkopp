{
    "key": "samsung",
    "url": "http://samsung.com/no",
    "targets": [
        {
            "type": "product",
            "pageId": {
                "elem": "#content .product-info-wrap",
                "path": false
            },
            "fetch": [
                {
                    "type": "container",
                    "elem": "#content",
                    "children": [
                        {
                            "type": "container",
                            "elem": ".product-info-wrap",
                            "name": "info",
                            "children": [
                                {
                                    "type": "data",
                                    "elem": ".product-title[itemprop=name]",
                                    "name": "title"
                                },
                                {
                                    "type": "data",
                                    "elem": ".product-code[itemprop=identifier]",
                                    "attr": "content",
                                    "name": "model"
                                },
                                {
                                    "type": "data",
                                    "elem": "#carousel-00 img",
                                    "attr": "data-media-desktop",
                                    "name": "imagesNormal"
                                },
                                {
                                    "type": "data",
                                    "elem": "#carousel-00 img",
                                    "attr": "data-img-link",
                                    "name": "imagesBig"
                                }
                            ]
                        },
                        {
                            "type": "container",
                            "elem": ".tech-spec-module .spec-list",
                            "name": "sections",
                            "children": [
                                {
                                    "type": "data",
                                    "elem": ".tit",
                                    "name": "section"
                                },
                                {
                                    "type": "container",
                                    "elem": ".spec",
                                    "name": "specs",
                                    "children": [
                                        {
                                            "type": "data",
                                            "elem": ".sub-tit",
                                            "name": "spec"
                                        },
                                        {
                                            "type": "data",
                                            "elem": ".desc",
                                            "name": "value"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
